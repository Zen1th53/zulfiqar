pkgname=klogger
pkgver=1.0
pkgrel=8
pkgdesc='A keystroke logger for the NT-series of Windows.'
url='http://ntsecurity.nu/toolbox/klogger/'
groups=('zulfiqar' 'zulfiqar-windows' 'zulfiqar-keylogger')
license=('custom')
arch=('any')
source=('http://ntsecurity.nu/downloads/klogger.exe')
sha512sums=('cd49eb066f35ab19f2a1b6fd0f7cf500fb0602d4')

package() {
  install -Dm 644 "$pkgname.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

