arch=('any')
makedepends=('git')
source=("git+https://github.com/Zulfiqar OS/$pkgname.git")
sha512sums=('SKIP')

package() {
  install -dm 755 "$pkgdir/usr/share/windows/$pkgname"

  cp -a * "$pkgdir/usr/share/windows/$pkgname"
}

