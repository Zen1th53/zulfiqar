pkgname=webshells
pkgver=46.e8e1a37
pkgrel=1
groups=('zulfiqar' 'zulfiqar-webapp' 'zulfiqar-backdoor')
pkgdesc='Web Backdoors.'
url='https://github.com/Zulfiqar OS/webshells'
arch=('any')
license=('custom:unknown')
makedepends=('git')
source=("git+https://github.com/Zulfiqar OS/$pkgname.git")
sha512sums=('SKIP')

pkgver() {
  cd $pkgname

  echo $(git rev-list --count HEAD).$(git rev-parse --short HEAD)
}

package() {
  cd $pkgname

  install -dm 755 "$pkgdir/usr/share/$pkgname"

  rm README.md

  cp -a --no-preserve=ownership * "$pkgdir/usr/share/$pkgname/"
}

